{
    "name": "cleaniquecoders/laravel-taskable",
    "description": "Built with Laravel Standalone Package Creator",
    "keywords": ["package", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\LaravelTaskable\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\LaravelTaskable\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "cleaniquecoders/blueprint-macro": "^2.0",
        "cleaniquecoders/laravel-helper": "^1.0",
        "cleaniquecoders/laravel-observers": "^1.0",
        "illuminate/support": "^5.5|^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\LaravelTaskable\\LaravelTaskableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
